{
    "name": "mamitech/laravel-slow-query-log",
    "description": "Slow query logging for Laravel",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Mamitech\\SlowQueryLog\\": "src/",
            "Mamitech\\SlowQueryLog\\Controllers\\": "src/Http/Controllers"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mamitech\\SlowQueryLog\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mamitech\\SlowQueryLog\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "orchestra/testbench": "7.x-dev|9.x-dev"
    }
}
