{
    "name": "vynhart/laravel-slow-query-log",
    "require": {
        "php": ">=7.1",
        "illuminate/support": ">=5.4.0"
    },
    "autoload": {
        "psr-4": {
            "Vynhart\\SlowQueryLog\\": "src/",
            "Vynhart\\SlowQueryLog\\Controllers\\": "src/Http/Controllers"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vynhart\\SlowQueryLog\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vynhart\\SlowQueryLog\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}